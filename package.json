{
    "name": "cvs-plugin",
    "publisher": "jaimelin",
    "repository": {
      "type": "git",
      "url": "https://github.com/Jaimecclin/cvs-pluging-vscode-ext"
    },
    "displayName": "CVS-plugin",
    "icon": "resources/ink-swirl-svgrepo-com.png",
    "description": "A CVS version control extension",
    "version": "0.1.4",
    "engines": {
      "vscode": "^1.65.0"
    },
    "categories": [
      "Other"
    ],
    "activationEvents": [
      "onCommand:cvs-plugin.helloWorld",
      "onCommand:cvs-plugin.status",
      "onCommand:cvs-plugin.cmdTest"
    ],
    "main": "./out/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "cvs-plugin.helloWorld",
          "title": "Hello World"
        },
        {
          "command": "cvs-plugin.status",
          "title": "cvs status",
          "icon": "resources/dark/refresh.svg"
        },
        {
          "command": "cvs-plugin.cmdTest",
          "title": "TestCommand"
        }
      ],
      "viewsContainers": {
        "activitybar": [
          {
            "id": "cvs-plugin",
            "title": "CVS-plugin",
            "icon": "resources/ink-swirl-svgrepo-com.svg",
            "contextualTitle": "CVS-plugin"
          }
        ]
      },
      "views": {
        "cvs-plugin": [
          {
            "id": "changed-files",
            "name": "Changed Files"
          },
          {
            "id": "view-2",
            "name": ""
          }
        ]
      },
      "menus": {
        "view/title": [
          {
            "command": "cvs-plugin.status",
            "when": "view == changed-files",
            "group": "navigation"
          },
          {
            "command": "cvs-plugin.cmdTest",
            "when": "view == changed-files",
            "group": "navigation"
          }
        ]
      }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.65.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4",
        "@vscode/test-electron": "^2.0.3"
    }
}
