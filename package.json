{
  "name": "cvs-plugin",
	"displayName": "CVS-plugin",
	"description": "a cvs version control extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:cvs-plugin.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "cvs-plugin.helloWorld",
            "title": "Hello World"

		}],
		"viewsContainers": {
			"activitybar": [
			  {
				"id": "cvs-plugin",
				"title": "CVS-plugin",
				"icon": "resources/banana-svgrepo-com.svg"
			  }
			]
		},
		"views": {
			"cvs-plugin": [
			  {
				"id": "changed-files",
				"name": "Changed Files"
			  },
			  {
				"id": "view-2",
				"name": "view-2"
			  }
			]
		  }
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.64.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4",
        "@vscode/test-electron": "^2.0.3"
    }

}
